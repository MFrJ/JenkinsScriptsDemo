pipeline {
	agent any
	stages{
		
		stage("Git-Checkout"){
			steps {
				echo "Checking out from Git Repo";
			}
		}
		
		stage("Build"){
			steps {
				echo "Building project";
			}
		}
		
		stage("Test"){
			steps {
				echo "Testing project";
			}
		}
		
		stage("Deploy"){
			steps {
				echo "Deploying project";
			}
		}
	
	
	}
}